<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.Jevin.Dao.StudentDao">

<select id="selectone" resultType="com.Jevin.pojo.Student" parameterType="String">
    select * from student where id=#{id}
</select>

    <select id="selectAll" resultType="com.Jevin.pojo.Student">
        select * from student
    </select>


</mapper>
